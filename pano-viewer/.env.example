# 环境配置示例文件
# 复制为 .env 并根据实际情况修改

# 应用环境 (development | production)
NODE_ENV=production

# 应用URL（部署后的域名）
APP_URL=https://your-domain.com

# CDN配置
CDN_URL=https://cdn.your-domain.com
USE_CDN=true

# Three.js CDN
THREEJS_CDN=https://cdn.jsdelivr.net/npm/three@0.160.0/build/three.module.js
EXRLOADER_CDN=https://cdn.jsdelivr.net/npm/three@0.160.0/examples/jsm/loaders/EXRLoader.js

# 性能优化
ENABLE_SERVICE_WORKER=true
ENABLE_GZIP=true
ENABLE_CACHE=true

# 功能开关
ENABLE_EXR_SUPPORT=true
ENABLE_HDR_SUPPORT=true
ENABLE_AUTO_SAVE=true
AUTO_SAVE_INTERVAL=30000

# 上传限制
MAX_FILE_SIZE=50
MAX_FILES_COUNT=20

# 调试模式
DEBUG=false
CONSOLE_LOG=false
